{
  "dependencies": {
    "@angular/animations": "^15.1.1"
  },
  "builds": [
    {
    "src": "ngfront/package.json",
    "use": "@vercel/npm",
    "config": {
        "command": "ng build --prod"
    }
    },
    {
    "src": "/requirements.txt",
    "use": "@vercel/python",
    "config": {
        "runtime": "python3.8"
    }
    }
],
"routes": [
    {
    "src": "/(.*)",
    "dest": "ngfront/src/index.html"
    },
    {
    "src": "/(.*)",
    "dest": "djback/manage.py runserver 0.0.0.0:${VERCEL_REGION_PORT}"
    }
]
}
